<Window x:Class="CWDM_Control_Board_GUI.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CWDM_Control_Board_GUI"
        mc:Ignorable="d"
        Background="#FF282828"
        Title="CWDM Control Board GUI" Height="450" Width="800"
        Icon="path132.png"
        >
    <Window.CommandBindings>
        <CommandBinding x:Name="PasswordCommand" Executed="PasswordCommand_Executed"/>
    </Window.CommandBindings>

    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition />
            <ColumnDefinition />
            <ColumnDefinition Width="180"/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition  />
            <RowDefinition Height="35"/>
        </Grid.RowDefinitions>
        <Grid Grid.Row="0" Grid.Column="0" Background="FloralWhite" Grid.ColumnSpan="2">

            <TabControl Name="MainControl" SelectedIndex="1" Grid.Row="0" IsEnabled="{Binding Path=ConnectionStatus}">
                <TabItem Name="Autotune_Heaters" Header="Autotune Heaters" >
                    <StackPanel VerticalAlignment="Bottom">
                        <Grid>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="300"/>
                                <ColumnDefinition Width="Auto"/>
                                <ColumnDefinition Width="Auto"/>
                                <ColumnDefinition Width="Auto"/>
                            </Grid.ColumnDefinitions>
                            <Button Content="MUX Left Channel" Name="MUX_Left" Width="140" Margin="5" Grid.Column="0" HorizontalAlignment="Right" Click="AutotuneMUXLeft"/>
                            <Ellipse Width="12" Height="12" Name="MUX_LeftGreen" Grid.Column="1" Fill="Gray" VerticalAlignment="Center" HorizontalAlignment="Left" Stroke="Gray" Margin="2"/>
                            <Ellipse Width="12" Height="12" Name="MUX_LeftOrange" Grid.Column="2" Fill="Gray" VerticalAlignment="Center" HorizontalAlignment="Left" Stroke="Gray" Margin="2"/>
                            <Ellipse Width="12" Height="12" Name="MUX_LeftRed" Grid.Column="3" Fill="Gray" VerticalAlignment="Center" HorizontalAlignment="Left" Stroke="Gray" Margin="2"/>
                        </Grid>
                        <Grid>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="300"/>
                                <ColumnDefinition Width="Auto"/>
                                <ColumnDefinition Width="Auto"/>
                                <ColumnDefinition Width="Auto"/>
                            </Grid.ColumnDefinitions>
                            <Button Content="MUX Right Channel" Name="MUX_Right" Width="140" Margin="5" Grid.Column="0" HorizontalAlignment="Right" Click="AutotuneMUXRight"/>
                            <Ellipse Width="12" Height="12" Name="MUX_RightGreen" Grid.Column="1" Fill="Gray" VerticalAlignment="Center" HorizontalAlignment="Left" Stroke="Gray" Margin="2"/>
                            <Ellipse Width="12" Height="12" Name="MUX_RightOrange" Grid.Column="2" Fill="Gray" VerticalAlignment="Center" HorizontalAlignment="Left" Stroke="Gray" Margin="2"/>
                            <Ellipse Width="12" Height="12" Name="MUX_RightRed" Grid.Column="3" Fill="Gray" VerticalAlignment="Center" HorizontalAlignment="Left" Stroke="Gray" Margin="2"/>
                        </Grid>
                        <Grid>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="300"/>
                                <ColumnDefinition Width="Auto"/>
                                <ColumnDefinition Width="Auto"/>
                                <ColumnDefinition Width="Auto"/>
                            </Grid.ColumnDefinitions>
                            <Button Content="DMUX Left Channel" Width="140" Margin="5" Grid.Column="0" HorizontalAlignment="Right" Click="AutotuneDMUXLeft"/>
                            <Ellipse Width="12" Height="12" Name="DMUX_LeftGreen" Grid.Column="1" Fill="Gray" VerticalAlignment="Center" HorizontalAlignment="Left" Stroke="Gray" Margin="2"/>
                            <Ellipse Width="12" Height="12" Name="DMUX_LeftOrange" Grid.Column="2" Fill="Gray" VerticalAlignment="Center" HorizontalAlignment="Left" Stroke="Gray" Margin="2"/>
                            <Ellipse Width="12" Height="12" Name="DMUX_LeftRed" Grid.Column="3" Fill="Gray" VerticalAlignment="Center" HorizontalAlignment="Left" Stroke="Gray" Margin="2"/>
                        </Grid>
                        <Grid>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="300"/>
                                <ColumnDefinition Width="Auto"/>
                                <ColumnDefinition Width="Auto"/>
                                <ColumnDefinition Width="Auto"/>
                            </Grid.ColumnDefinitions>
                            <Button Content="DMUX Right Channel" Width="140" Margin="5" Grid.Column="0" HorizontalAlignment="Right" Click="AutotuneDMUXRight"/>
                            <Ellipse Width="12" Height="12" Name="DMUX_RightGreen" Grid.Column="1" Fill="Gray" VerticalAlignment="Center" HorizontalAlignment="Left" Stroke="Gray" Margin="2"/>
                            <Ellipse Width="12" Height="12" Name="DMUX_RightOrange" Grid.Column="2" Fill="Gray" VerticalAlignment="Center" HorizontalAlignment="Left" Stroke="Gray" Margin="2"/>
                            <Ellipse Width="12" Height="12" Name="DMUX_RightRed" Grid.Column="3" Fill="Gray" VerticalAlignment="Center" HorizontalAlignment="Left" Stroke="Gray" Margin="2"/>
                        </Grid>
                        <Button Content="Abort" Width="100" Margin="0,100,0,10" Click="Abort"/>
                    </StackPanel>
                </TabItem>
                <TabItem Header="Register Map">
                    <Grid>
                        <Grid.RowDefinitions>
                            <RowDefinition/>
                            <RowDefinition Height="25"/>
                        </Grid.RowDefinitions>
                        <TabControl>
                            <TabItem Header="ADC Signal">
                                <DataGrid Name="ADC_Registers" RowHeight="30" ColumnWidth="125" HorizontalAlignment="Left" AutoGenerateColumns="False" >
                                    <DataGrid.Columns>
                                        <DataGridTextColumn Header="Register" Binding="{Binding Name}"/>
                                        <DataGridTemplateColumn Header="Value">
                                            <DataGridTemplateColumn.CellTemplate>
                                                <DataTemplate>
                                                    <TextBlock Text="{Binding RegisterString}">
                                                        <TextBlock.ToolTip>
                                                            <ToolTip Content="{Binding ErrorText}"/>
                                                        </TextBlock.ToolTip>
                                                    </TextBlock>
                                                </DataTemplate>
                                            </DataGridTemplateColumn.CellTemplate>
                                        </DataGridTemplateColumn>
                                    </DataGrid.Columns>


                                </DataGrid>
                            </TabItem>
                            <TabItem Header="DAC Signal">
                                <DataGrid Name="DAC_Registers" RowHeight="30" ColumnWidth="120" HorizontalAlignment="Left" AutoGenerateColumns="False">
                                    <DataGrid.Columns>
                                        <DataGridTextColumn Header="Register" Binding="{Binding Name}"/>
                                        <DataGridTemplateColumn Header="Output Value">
                                            <DataGridTemplateColumn.CellTemplate>
                                                <DataTemplate>
                                                    <TextBlock Text="{Binding OutputString}">
                                                        <TextBlock.ToolTip>
                                                            <ToolTip Content="{Binding OutputErrorText}"/>
                                                        </TextBlock.ToolTip>
                                                    </TextBlock>
                                                </DataTemplate>
                                            </DataGridTemplateColumn.CellTemplate>
                                        </DataGridTemplateColumn>
                                        <DataGridTemplateColumn Header="Offset Value">
                                            <DataGridTemplateColumn.CellTemplate>
                                                <DataTemplate>
                                                    <TextBlock Text="{Binding OffsetString}">
                                                        <TextBlock.ToolTip>
                                                            <ToolTip Content="{Binding OffsetErrorText}"/>
                                                        </TextBlock.ToolTip>
                                                    </TextBlock>
                                                </DataTemplate>
                                            </DataGridTemplateColumn.CellTemplate>
                                        </DataGridTemplateColumn>
                                        <DataGridTemplateColumn Header="Gain Value">
                                            <DataGridTemplateColumn.CellTemplate>
                                                <DataTemplate>
                                                    <TextBlock Text="{Binding GainString}">
                                                        <TextBlock.ToolTip>
                                                            <ToolTip Content="{Binding GainErrorText}"/>
                                                        </TextBlock.ToolTip>
                                                    </TextBlock>
                                                </DataTemplate>
                                            </DataGridTemplateColumn.CellTemplate>
                                        </DataGridTemplateColumn>
                                    </DataGrid.Columns>

                                </DataGrid>
                            </TabItem>
                            <TabItem Header="Select Signal">
                                <DataGrid Name="SEL_Registers" RowHeight="30" ColumnWidth="130" HorizontalAlignment="Left" AutoGenerateColumns="False" >
                                    <DataGrid.Columns>
                                        <DataGridTextColumn Header="Register" Binding="{Binding Name}"/>
                                        <DataGridTemplateColumn Header="Value">
                                            <DataGridTemplateColumn.CellTemplate>
                                                <DataTemplate>
                                                    <TextBlock Text="{Binding RegisterString}">
                                                        <TextBlock.ToolTip>
                                                            <ToolTip Content="{Binding ErrorText}"/>
                                                        </TextBlock.ToolTip>
                                                    </TextBlock>
                                                </DataTemplate>
                                            </DataGridTemplateColumn.CellTemplate>
                                        </DataGridTemplateColumn>
                                    </DataGrid.Columns>
                                </DataGrid>
                            </TabItem>
                        </TabControl>
                        <Grid Grid.Row="1">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="250"/>
                                <ColumnDefinition />
                                <ColumnDefinition/>
                                <ColumnDefinition/>
                            </Grid.ColumnDefinitions>
                            <UniformGrid Grid.Column="0" Columns="3" Margin="20,0,0,0">
                                <RadioButton VerticalAlignment="Center" GroupName="BaseData" Content="Binary" Checked="BinaryButton_Checked"/>
                                <RadioButton VerticalAlignment="Center" GroupName="BaseData" Content="Hex" Checked="HexButton_Checked" IsChecked="True"/>
                                <RadioButton VerticalAlignment="Center" GroupName="BaseData" Content="Decimal"  Checked="DecimalButton_Checked"/>
                            </UniformGrid>
                            <Grid Grid.Column="1" HorizontalAlignment="Center">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition/>
                                </Grid.ColumnDefinitions>
                                <CheckBox Name="AutoRefresh" VerticalAlignment="Bottom" Grid.Column="0" Margin="0,0,0,5" Unchecked="AutoRefresh_Unchecked" Checked="AutoRefresh_Checked"/>
                                <Label Content="AutoRefresh" Grid.Column="1" />

                            </Grid>
                            <Button Name="RefreshButton" Grid.Column="2" Width="100" Height="20" VerticalAlignment="Center" Content="Refresh Registers" HorizontalAlignment="Right" Margin="0,2,10,0" Click="RefreshButton_Click" Visibility="{Binding RefreshButtonVisibility}" IsEnabled="{Binding RefreshButtonEnabled}"/>
                            <ProgressBar Grid.Column="3" Name="ReadProgress" Width="100" Height="15" VerticalAlignment="Center" HorizontalAlignment="Left" Margin="0,2,0,0"/>
                        </Grid>
                    </Grid>
                </TabItem>
                <TabItem Header="Board Scripting Language" IsEnabled="{Binding Path=SuperuserAccess}">
                    <StackPanel>
                        <RichTextBox Name="CodeBox" Width="500" Height="200" HorizontalAlignment="Left" VerticalAlignment="Top" FontFamily="Consolas" AcceptsTab="True" Margin="0,0,0,10" TextChanged="CodeBox_TextChanged" VerticalScrollBarVisibility="Auto">
                            <RichTextBox.Resources>
                                <Style TargetType="{x:Type Paragraph}">
                                    <Setter Property="Margin" Value="0"/>
                                </Style>
                            </RichTextBox.Resources>
                        </RichTextBox>
                        <Separator />
                        <RichTextBox Name="OutputBox" Width="500" Height="100" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="0,10,0,0" VerticalScrollBarVisibility="Auto">
                            <RichTextBox.Resources>
                                <Style TargetType="{x:Type Paragraph}">
                                    <Setter Property="Margin" Value="0"/>
                                </Style>
                            </RichTextBox.Resources>
                        </RichTextBox>
                        <UniformGrid>
                            <Button Name="CompileButton" Height="20" Width="80" Content="Compile" HorizontalAlignment="Right" Click="Compile_Click"/>
                            <Button Name="RunButton" Height="20" Width="80" HorizontalAlignment="Left" Content="Run" Click="RunButton_Click" />
                        </UniformGrid>

                    </StackPanel>
                </TabItem>
                <TabItem Header="Misc." IsEnabled="False"/>
            </TabControl>


        </Grid>
        <Grid Grid.Row="0" Grid.Column="2" Background="#FF0F2572">
            <Grid.RowDefinitions>
                <RowDefinition/>
                <RowDefinition Height="Auto"/>
            </Grid.RowDefinitions>
            <Grid Grid.Row="1" IsEnabled="{Binding Path=ConnectionStatus}">
                <Border BorderBrush="Black" BorderThickness="2">
                    <StackPanel>
                        <StackPanel.Background>
                            <SolidColorBrush Color="{DynamicResource {x:Static SystemColors.GradientInactiveCaptionColorKey}}"/>
                        </StackPanel.Background>
                        <Label Content="Register Write" HorizontalAlignment="Center"/>
                        <UniformGrid>
                            <Label Content="Reg"/>
                            <TextBox Name="RegisterBox" VerticalContentAlignment="Center"/>
                            <Label Content="Value"/>
                            <TextBox Name="ValueBox" VerticalContentAlignment="Center"/>
                        </UniformGrid>
                        <Button Content="Write" Width="70" Name="WriteButton" Click="WriteButton_Click" />
                    </StackPanel>
                </Border>
            </Grid>
